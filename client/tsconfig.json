{
  "compilerOptions": {
    "jsx": "react",
    "target": "es5",
    "moduleResolution": "node",
    "module": "esnext",
    "strict": true,
    "importHelpers": true,
    "preserveConstEnums": true,
    "noImplicitAny": true,
    "sourceMap": true,
    "allowSyntheticDefaultImports": true,
    "lib": ["dom", "es6", "es2015.promise"]
  },
  "include": ["./src/**/*"]
}
